{
"compilerOptions": {
  "esModuleInterop": true,
  "forceConsistentCasingInFileNames": true,
  "module": "commonjs",
  "skipLibCheck": false,
  "sourceMap": true,
  "strict": true,
  "strictPropertyInitialization": false,
  "target": "es2017",
  "useUnknownInCatchVariables": false,
},
"include": [
  "src/**/*.ts"
],
"exclude": ["node_modules", "src/**/*.spec.ts"]
}
